	<locale key="fr_FR" name="Français (France)"/>
	<locale key="it_IT" name="Italiano"/>
	<locale key="pt_BR" name="Português (Brasil)"/>
	<locale key="pt_PT" name="Português (Portugal)"/>
	<locale key="zh_TW" name="正體中文"/>
</locales>
